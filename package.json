{
  "name": "squad-server-info-discord-bot",
  "version": "1.0.0",
  "description": "Discord Bot to show squad server information",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "test": "ts-mocha -n loader=ts-node/esm/transpile-only -p tsconfig.json 'test/*.test.ts'",
    "format": "prettier --write \"src/**/*.ts\"",
    "serve": "node build/main.js",
    "start": "node --loader ts-node/esm/transpile-only src/main.ts",
    "dev": "nodemon --exec node --loader ts-node/esm/transpile-only src/main.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "storage/*"
    ]
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "squad"
  ],
  "author": "Mikerosoft25",
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@fabricio-191/valve-server-query": "^4.1.8",
    "config": "^3.3.8",
    "discord.js": "^14.7.1",
    "discordx": "^11.5.2",
    "luxon": "^3.2.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/config": "^3.3.0",
    "@types/expect": "^24.3.0",
    "@types/luxon": "^3.2.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
